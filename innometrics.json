{
    "version_name": "userAgentInno",
    "version_description": "Innometrics app to automatically add browser info to the session",
    "pages": [{
        "name": "App Settings",
        "type": "bundled",
        "content": "frontend/settings.html"
    }, {
        "name": "Profile Stream",
        "type": "bundled",
        "content": "frontend/stream.html"
    }],
    "permissions": ["stream", "profile.update", "profile.read", "app.read.own"],
    "runtime": {
        "language": "node",
        "version": "0.0.1"
    }
}
